<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- customer has a property name "address" -->
	<bean id="customer"
		class="com.yinzifan.liandisys._0921_SpringAutoWire02ByName_Demo.bean.Customer"
		autowire="byName" />

	<bean id="address"
		class="com.yinzifan.liandisys._0921_SpringAutoWire02ByName_Demo.bean.Address">
		<property name="fullAddress" value="nj" />
	</bean>

</beans>
